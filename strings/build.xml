<?xml version="1.0" ?>

<project default="run">
  <property name="chapter" value="strings"/>
  <property name="excludedfiles" value=""/>
  <import file="../Ant-Common.xml"/>

  <target name="run" description="Compile and run" depends="build">
    <jrun cls="ArrayListDisplay" />
    <jrun cls="BetterRead" />
    <jrun cls="Concatenation" />
    <jrun cls="Conversion" />
    <jrun cls="DatabaseException" />
    <jrun cls="Finding" />
    <jrun cls="Groups" />
    <jrun cls="Immutable" />
    <jrun cls="InfiniteRecursion" failOnError='false' msg='* Exception was Expected *' />
    <jrun cls="IntegerMatch" />
    <jrun cls="JGrep" arguments='JGrep.java "\\b[Ssct]\\w+"' />
    <jrun cls="Receipt" />
    <jrun cls="ReFlags" />
    <jrun cls="Replacing" />
    <jrun cls="ReplacingStringTokenizer" />
    <jrun cls="Resetting" />
    <jrun cls="Rudolph" />
    <jrun cls="ScannerDelimiter" />
    <jrun cls="SimpleFormat" />
    <jrun cls="SimpleRead" />
    <jrun cls="SplitDemo" />
    <jrun cls="Splitting" />
    <jrun cls="StartEnd" />
    <jrun cls="TestRegularExpression" arguments='abcabcabcdefabc "abc+" "(abc)+" "(abc){2,}" ' />
    <jrun cls="TheReplacements" />
    <jrun cls="ThreatAnalyzer" />
    <jrun cls="Turtle" />
    <jrun cls="UsingStringBuilder" />
  </target>

</project>
