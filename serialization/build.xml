<?xml version="1.0" ?>

<project default="run">
  <property name="chapter" value="serialization"/>
  <property name="excludedfiles" value=""/>
  <import file="../Ant-Common.xml"/>
  <import file="../Ant-Clean.xml"/>

  <target name="run" description="Compile and run" depends="build">
    <jrun cls="APerson" />
    <jrun cls="AStoreCADState" />
    <jrun cls="Blip3" />
    <jrun cls="Blips" />
    <jrun cls="FreezeAlien" />
    <jrun cls="Logon" failOnError='false' />
    <jrun cls="MyWorld" />
    <jrun cls="People" />
    <jrun cls="RecoverCADState" />
    <jrun cls="SerialCtl" />
    <jrun cls="Worm" />
    <jrun cls="serialization.xfiles.ThawAlien" dirpath="../serialization/xfiles" />
  </target>

</project>
