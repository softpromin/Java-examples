<?xml version="1.0" ?>

<project default="run">
  <property name="chapter" value="onjava"/>
  <property name="excludedfiles" value=""/>
  <import file="../Ant-Common.xml"/>
  <import file="../Ant-Clean.xml"/>

  <target name="run" description="Compile and run" depends="build">
    <jrun cls="onjava.CollectionMethodDifferences" dirpath="../onjava" />
    <jrun cls="onjava.CountingFilledMap" dirpath="../onjava" />
    <jrun cls="onjava.CountingIntegerList" dirpath="../onjava" />
    <jrun cls="onjava.Countries" dirpath="../onjava" />
